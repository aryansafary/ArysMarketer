<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/CenterListItemClick"
    android:background="@drawable/cover_stroke_dont_have_color"
    xmlns:app="http://schemas.android.com/apk/res-auto">
<com.google.android.material.textview.MaterialTextView
    android:id="@+id/item_centerListName"
    android:layout_margin="10dp"
    android:layout_width="wrap_content"
    android:layout_height="0dp"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    android:textStyle="bold"
    style="@style/TextAppearance.AppCompat.Title"
    android:text="@string/ArysCompany"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/itm_centerListForm1"
        android:layout_margin="2dp"
        android:padding="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toTopOf="@id/itm_centerListForm1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/item_centerListName">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/item_centerList_province_text"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/b_nazanin"
            style="@style/TextAppearance.Material3.BodyLarge"
            android:layout_marginRight="10dp"
            android:text="@string/province"
            android:textColor="@color/Oli"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintLeft_toRightOf="@id/item_centerList_province"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/item_centerList_province"
            style="@style/TextAppearance.Material3.BodyMedium"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="10dp"
            android:fontFamily="@font/b_nazanin"
            android:text="@string/province"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toLeftOf="@id/item_centerList_province_text"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.666" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/itm_centerListForm2"
        android:layout_margin="2dp"
        android:padding="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toTopOf="@id/itm_centerListForm3"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/itm_centerListForm1">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/item_centerList_city_text"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/b_nazanin"
            style="@style/TextAppearance.Material3.BodyLarge"
            android:layout_marginRight="10dp"
            android:text="@string/city"
            android:textColor="@color/Oli"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintLeft_toRightOf="@id/item_centerList_city"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/item_centerList_city"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            style="@style/TextAppearance.Material3.BodyMedium"
            android:layout_marginRight="10dp"
            android:fontFamily="@font/b_nazanin"
            android:text="@string/city"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@id/item_centerList_city_text"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/itm_centerListForm3"
        android:layout_margin="2dp"
        android:padding="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toTopOf="@id/itm_centerListForm4"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/itm_centerListForm2">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/item_centerList_location_text"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/b_nazanin"
            style="@style/TextAppearance.Material3.BodyLarge"
            android:layout_marginRight="10dp"
            android:text="@string/Address"
            android:textColor="@color/Oli"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintLeft_toRightOf="@id/item_centerList_location"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/item_centerList_location"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            style="@style/TextAppearance.Material3.BodyMedium"
            android:layout_marginRight="10dp"
            android:fontFamily="@font/b_nazanin"
            android:text="@string/Address"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@id/item_centerList_location_text"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/itm_centerListForm4"
        android:layout_margin="2dp"
        android:padding="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toTopOf="@id/itm_centerListForm5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/itm_centerListForm3">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/item_centerList_phone_text"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/b_nazanin"
            style="@style/TextAppearance.Material3.BodyLarge"
            android:layout_marginRight="10dp"
            android:text="@string/phone_farsi"
            android:textColor="@color/Oli"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintLeft_toRightOf="@id/item_centerList_phone"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/item_centerList_phone"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            style="@style/TextAppearance.Material3.BodyMedium"
            android:layout_marginRight="10dp"

            android:fontFamily="@font/b_nazanin"
            android:text="@string/phone_farsi"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@id/item_centerList_phone_text"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/itm_centerListForm5"
        android:layout_margin="2dp"
        android:padding="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/itm_centerListForm4">

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/item_centerList_activity_text"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:fontFamily="@font/b_nazanin"
            style="@style/TextAppearance.Material3.BodyLarge"
            android:layout_marginRight="10dp"
            android:text="@string/activity"
            android:textColor="@color/Oli"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintLeft_toRightOf="@id/item_centerList_activity"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.google.android.material.textview.MaterialTextView
            android:id="@+id/item_centerList_activity"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            style="@style/TextAppearance.Material3.BodyMedium"
            android:layout_marginRight="10dp"
            android:fontFamily="@font/b_nazanin"
            android:text="@string/activity"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintRight_toLeftOf="@id/item_centerList_activity_text"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>
<com.google.android.material.textview.MaterialTextView
    android:visibility="gone"
    android:id="@+id/item_centerList_id"
    android:layout_width="match_parent"
    android:layout_height="match_parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>